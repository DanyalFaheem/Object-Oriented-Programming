#include"attributeGraph1.h"
#include"SimpleGraph.h"
#include <iostream>
using namespace std;

int main() {
	attributeGraph1 obj;
	for(int counter = 0; counter < 5; counter++){
			cout << "Enter value for nodeid" << counter + 1 << endl;
			cin >> arr[counter];
		}
		obj.addnode(arr[0]);
		obj.addnode(arr[1]);
		obj.addnode(arr[2]);
		obj.addnode(arr[3]);
		obj.addnode(arr[4]);
		obj.addEdge(arr[0], arr[1]);
		obj.addEdge(arr[1], arr[2]);
		obj.addEdge(arr[2], arr[1]);
		obj.addEdge(arr[1], arr[3]);
	return 0;
}
